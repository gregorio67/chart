<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="students">
	<select id="selStudents" parameterType="hashMap" resultType="linkedHashMap">
		SELECT
			StudentId,
			StudentName,
			DepartmentId,
			DateOfBirth
		FROM STUDENTS
		WHERE 1 = 1
	</select>
	
</mapper>